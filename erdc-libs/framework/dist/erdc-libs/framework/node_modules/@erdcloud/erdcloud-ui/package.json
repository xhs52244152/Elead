{
  "name": "@erdcloud/erdcloud-ui",
  "version": "1.5.1",
  "module": "lib/erdcloud-ui.es.js",
  "main": "lib/erdcloud-ui.umd.js",
  "scripts": {
    "doc": "npm run serve",
    "serve": "vuepress dev ./docs --no-cache",
    "install:quill-editor": "cd ./packages/quill-editor && npm install",
    "install:ucd": "cd ./packages/ucd && npm install",
    "build": "npm run build:lib && npm run build:editor && npm run build:theme",
    "build:lib": "cross-env LIB_ENTRY=erdcloud-ui vite build",
    "build:editor": "cross-env LIB_ENTRY=editor vite build",
    "build:icon": "node ./packages/icon/src/index.js",
    "build:ucd": "node ./packages/ucd/build.js",
    "build:theme": "npm run build:icon && npm run build:ucd && node build/build.theme",
    "build:docs": "vuepress build ./docs -no-cache && node ./scripts/moveDist.js",
    "code-fix": "npx eslint-auto-fix --verbose --fix-on-startup ./src/**/*.js ./packages/**/*.vue ./packages/**/*.js ./examples/**/*.vue  ./examples/**/*.js",
    "changelog": "npx conventional-changelog-cli -p angular -i CHANGELOG.md -s",
    "postinstall": "patch-package"
  },
  "files": [
    "build",
    "lib",
    "src",
    "jsconfig.json"
  ],
  "devDependencies": {
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-transform-optional-chaining": "^7.21.0",
    "@babel/runtime-corejs3": "^7.24.1",
    "@vitejs/plugin-vue2": "^2.3.1",
    "@vitejs/plugin-vue2-jsx": "^1.1.1",
    "@vue/babel-preset-app": "^5.0.8",
    "@vuepress/plugin-active-header-links": "^1.9.8",
    "@vuepress/plugin-back-to-top": "^1.9.8",
    "axios": "^1.6.8",
    "babel-plugin-import": "^1.13.8",
    "core-js": "^3.36.1",
    "cross-env": "^7.0.3",
    "css-es-modules": "^1.0.2",
    "css-loader": "^5.2.7",
    "cssnano": "^5.1.10",
    "eslint": "^8.57.0",
    "eslint-plugin-html": "^8.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-vue": "^9.25.0",
    "fs-extra": "^11.2.0",
    "onchange": "^7.1.0",
    "postcss": "^8.4.27",
    "postcss-at-rules-variables": "^0.3.0",
    "postcss-bem": "^0.4.1",
    "postcss-bem-fix": "^2.1.0",
    "postcss-bem-linter": "^3.3.0",
    "postcss-cli": "^10.1.0",
    "postcss-each": "^1.1.0",
    "postcss-extend-rule": "^4.0.0",
    "postcss-for": "^2.1.1",
    "postcss-functions": "^4.0.2",
    "postcss-import": "^14.1.0",
    "postcss-import-ext-glob": "^2.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-nesting": "^10.1.7",
    "postcss-preset-env": "^7.7.0",
    "postcss-pxtorem": "^6.0.0",
    "postcss-safe-parser": "^6.0.0",
    "postcss-url": "^10.1.3",
    "postcss-use": "^3.0.0",
    "prettier": "^3.2.5",
    "quill1-table": "^1.7.2",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "vite": "^5.2.7",
    "vue": "~2.7.16",
    "vue-loader": "^15.10.0",
    "vue-server-renderer": "~2.7.16",
    "vue-template-compiler": "~2.7.16",
    "vuepress": "^1.9.10",
    "vuepress-plugin-demo-container": "^0.2.0",
    "vuepress-plugin-smooth-scroll": "^0.0.10",
    "vuepress-theme-vt": "^0.15.1",
    "webpack": "^4.46.0"
  },
  "dependencies": {
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "element-ui": "^2.15.14",
    "interactjs": "^1.10.17",
    "is-promise": "^4.0.0",
    "patch-package": "^8.0.0",
    "quill": "^1.3.7",
    "quill-image-uploader": "1.2",
    "sortablejs": "^1.15.0",
    "throttle-debounce": "^1.0.1",
    "vue-quill-editor": "^3.0.6",
    "vue-virtual-scroll-list": "^2.3.5",
    "vxe-table": "^3.8.7",
    "vxe-table-plugin-element": "^3.3.1",
    "xe-utils": "^3.5.4"
  }
}
